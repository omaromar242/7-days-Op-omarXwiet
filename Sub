G2L["c3"]["FontFace"] = Font.new([[rbxasset://fonts/families/FredokaOne.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal);
G2L["c3"]["TextColor3"] = Color3.fromRGB(0, 0, 0);
G2L["c3"]["BackgroundTransparency"] = 1;
G2L["c3"]["Size"] = UDim2.new(0.10324, 0, 0.04717, 0);
G2L["c3"]["Visible"] = false;
G2L["c3"]["BorderColor3"] = Color3.fromRGB(0, 0, 0);
G2L["c3"]["Text"] = [[Day]];
G2L["c3"]["Name"] = [[day]];
G2L["c3"]["Position"] = UDim2.new(0.44385, 0, 0.01032, 0);


-- StarterGui.omarXwiet.sigma.EspFrame.second.soundclick
local function C_1d()
